{
    "$schema": "https://json.schemastore.org/template",
    "author": "Vijay Anand E G",
    "defaultName": "BlazorWpf",
    "classifications": [
        "Windows",
        "Desktop",
        "WPF",
        "Blazor"
    ],
    "identity": "VijayAnand.Wpf.Blazor",
    "description": "A project for hosting Blazor component in the WPF desktop application",
    "name": "WPF App with Blazor component (.NET 6 / .NET MAUI)",
    "shortName": "wpf-blazor",
    "sourceName": "BlazorWpf",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "preferNameDirectory": true,
    "guids": [
        "F7B43479-A7C7-4021-BDD2-2C7F90604B4A"
    ],
    "symbols": {
        "HostIdentifier": {
            "type": "bind",
            "binding": "HostIdentifier"
        }
    },
    "sources": [
        {
            "source": "./",
            "target": "./",
            "copyOnly": [
                "**/*.eot",
                "**/*.svg",
                "**/*.ttf",
                "**/*.woff",
                "**/*.woff2",
                "**/*.min.css",
                "**/*.min.css.map",
                "**/*.min.js"
            ],
            "modifiers": [
                {
                    "condition": "(!(HostIdentifier == \"dotnetcli\" || HostIdentifier == \"dotnetcli-preview\"))",
                    "exclude": [
                        "BlazorWpf.sln"
                    ]
                }
            ]
        }
    ]
}
